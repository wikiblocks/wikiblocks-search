#!/usr/bin/env node

var app = require('../main.js');

/**
 * Get port from environment and store in Express.
 */

var port = normalizePort('8081');

app.listen(port);

/**
 * Create HTTP server.
 */


/**
 * Normalize a port into a number, string, or false.
 */
function normalizePort(val) {
  var port = parseInt(val, 10);

  if (isNaN(port)) {
    // named pipe
    return val;
  }

  if (port >= 0) {
    // port number
    return port;
  }

  return false;
}


